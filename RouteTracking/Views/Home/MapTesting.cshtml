@model Tuple<double, double>

@{
    ViewBag.Title = "Map Testing";
}

<h2>@ViewBag.Title</h2>

    <style>
        #map {
            height: 600px;
        }
    </style>

<div id="map"></div>
<div id="capture"></div>

<script>
    var map;

    function initMap() {
        map = new google.maps.Map(document.getElementById('map'),
        {
            center: { lat: @Model.Item1, lng: @Model.Item2 },
            zoom: 8
        });
    }
</script>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBq8Ut_hUTFpkP0FLCi_6VgIlhl8pKUyvs&callback=initMap" async defer></script>